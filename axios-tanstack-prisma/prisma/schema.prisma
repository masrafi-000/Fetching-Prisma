

generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Description {
  id Int @id @default(autoincrement())
  title String @unique
  description String @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}